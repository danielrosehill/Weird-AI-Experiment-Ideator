# OpenRouter API Configuration
OPENROUTER_API_KEY=your_api_key_here

# Model Configuration (use cheap, creative models from OpenRouter)
# Recommended: google/gemini-2.0-flash-exp for structured JSON output
# This model excels at constrained output and has a large context window
# Alternatives:
# - meta-llama/llama-3.1-8b-instruct (very cheap, good creativity)
# - google/gemini-flash-1.5-8b (fast and cheap)
# - anthropic/claude-3.5-haiku (balance of quality and cost)

GENERATOR_MODEL=google/gemini-2.0-flash-exp
EVALUATOR_MODEL=google/gemini-2.0-flash-exp
CATEGORIZER_MODEL=google/gemini-2.0-flash-exp
SYNTHESIZER_MODEL=google/gemini-2.0-flash-exp

# Generation Parameters
NUM_IDEAS=50
TEMPERATURE=0.9  # Higher temperature for more creativity
